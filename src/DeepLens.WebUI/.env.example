# DeepLens WebUI Environment Configuration
# Using HTTP for development to avoid certificate issues

# Identity API (Authentication & Authorization)
VITE_IDENTITY_API_URL=http://localhost:5198

# Search API (Image Upload & Search)
VITE_API_BASE_URL=http://localhost:5000
VITE_SEARCH_API_URL=http://localhost:5000

# Admin API (Optional - not currently running)
VITE_ADMIN_API_URL=http://localhost:5205

# Feature Extraction API (Python ML Service)
VITE_FEATURE_API_URL=http://localhost:8001

# Application Metadata
VITE_APP_NAME=DeepLens
VITE_APP_VERSION=0.1.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
