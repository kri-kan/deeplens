{
    "name": "whatsapp-community-processor",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "build:client": "cd client && npm run build",
        "build:all": "npm run build:client && npm run build",
        "start": "node dist/index.js",
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "ts-node-dev --respawn --transpile-only src/index.ts",
        "dev:client": "cd client && npm run dev",
        "setup": "npm install && cd client && npm install && npm run build && cd .. && npm run build",
        "reset": "powershell -ExecutionPolicy Bypass -File ./reset-all.ps1"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@hapi/boom": "^10.0.1",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/auto-instrumentations-node": "^0.67.3",
        "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
        "@opentelemetry/resources": "^2.2.0",
        "@opentelemetry/sdk-metrics": "^2.2.0",
        "@opentelemetry/sdk-node": "^0.208.0",
        "@opentelemetry/sdk-trace-node": "^2.2.0",
        "@opentelemetry/semantic-conventions": "^1.38.0",
        "@whiskeysockets/baileys": "^7.0.0-rc.9",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "ioredis": "^5.8.2",
        "kafkajs": "^2.2.4",
        "minio": "^8.0.3",
        "pg": "^8.13.1",
        "pino": "^9.6.0",
        "pino-roll": "^4.0.0",
        "qrcode-terminal": "^0.12.0",
        "socket.io": "^4.8.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.0",
        "@types/minio": "^7.1.1",
        "@types/node": "^22.10.2",
        "@types/pg": "^8.11.10",
        "@types/qrcode-terminal": "^0.12.2",
        "concurrently": "^9.2.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.2"
    }
}
