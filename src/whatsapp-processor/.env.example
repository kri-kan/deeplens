# WhatsApp Session Configuration
SESSION_ID=community_alpha
TENANT_NAME=YourTenantName

# Data Directory
DATA_DIR=./data

# MinIO Configuration
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=tenant-<your-tenant-uuid>

# Database Configuration
# Note: Special characters in password should be URL encoded (! becomes %21)
DB_CONNECTION_STRING=postgresql://postgres:YourPassword@localhost:5432/tenant_YourTenant_metadata

# Server Configuration
API_PORT=3005
LOG_LEVEL=info
